{
  "name": "web-boilerplate",
  "version": "1.0.0",
  "description": "boiler plater for freelance web projects",
  "main": "index.js",
  "scripts": {
    "backend:build": "nodemon app.js",
    "backend:development": "nodemon app.js",
    "frontend:build": "webpack --progress --config webpack.build.config.js",
    "frontend:development": "webpack serve --progress --config webpack.development.config.js",
    "start": "concurrently --kill-others \"npm run backend:development\" \"npm run frontend:development\""
  },
  "keywords": [
    "webpack"
  ],
  "author": "Harpreet",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "imagemin": "^8.0.1",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^8.0.0",
    "postcss-loader": "^7.0.2",
    "pug": "^3.0.2",
    "raw-loader": "^4.0.2",
    "sass-loader": "^13.2.0",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@prismicio/client": "^6.7.3",
    "@prismicio/helpers": "^2.3.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-fetch": "^2.6.9",
    "prismic-dom": "^2.2.7"
  }
}
